<span class="name">
  Custom Prefix
</span>
<input id="prefix" class="prefix" placeholder="<%= guild.prefix ? guild.prefix : '!' %>" maxlength="3"/>
<span id="save-switch-default-save" class="save">
  SAVE
</span>
<script type="text/javascript">
  $(document).on('click','#save-switch-default-save',function(e){
      e.preventDefault()
      $.ajax({
          type: "POST",
          url: "/api/<%= server.id %>/<%= plugin %>/prefix",
          data: {
              prefix: $("#prefix").val(),
          },
          success: function(result) {
              alert('ok')
          },
          error: function(result) {
              alert(result)
              console.log(result)
          }
      })
  })
</script>
